    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="MyButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/button_color</item>
        <item name="android:textColor">@color/button_text_color</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/padding_margin_layout_double</item>
        <item name="android:paddingRight">@dimen/padding_margin_layout_double</item>
        <item name="android:paddingTop">@dimen/padding_margin_layout</item>
        <item name="android:paddingBottom">@dimen/padding_margin_layout</item>
        <item name="android:layout_margin">@dimen/padding_margin_layout</item>
        <item name="android:textSize">@dimen/button_text_size</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="MyCardView" parent="CardView.Light">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/padding_margin_layout</item>
        <!--<item name="app:cardCornerRadius">@dimen/cardview_radius</item>-->
    </style>
    <style name="MyEditTextStyle" parent="Base.V7.Widget.AppCompat.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/primery_text_size</item>
        <item name="android:layout_height">wrap_content</item>
        <!--<item name="android:background">@drawable/rounded_corners_text_view</item>-->
        <item name="android:layout_marginTop">@dimen/padding_margin_layout</item>
        <item name="android:backgroundTint">@color/text_view_bottom_line_color</item>
        <item name="android:paddingBottom">@dimen/spacing_between_line_and_text</item>
        <item name="android:maxLines">1</item>
        <item name="android:textCursorDrawable">@null</item>
    </style>
    <style name="MySpinnerStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <!--<item name="android:background">@drawable/rounded_corners_text_view</item>-->
    </style>
    <style name="MyTextInputLayoutStyle" parent="Widget.Design.TextInputLayout">
        <item name="android:textSize">@dimen/primery_text_size</item>
        <item name="android:textColor">@color/colorAccent</item>
    </style>
